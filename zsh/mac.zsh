# ls
alias ls="ls -FGh"
alias ll="ls -lFGh"
alias la="ls -aFGh"
alias lla="ls -laFGh"

alias updatedb="/usr/libexec/locate.updatedb"

alias nproc="sysctl -n hw.ncpu"

alias date="date +%Y-%m-%dT%H:%M:%S%z"
alias date2="date +%Y-%m-%dT%r%z"
alias today="command date +%Y-%m-%d"
alias now="command date +%H:%M:%S%z"
