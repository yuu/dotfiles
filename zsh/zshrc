# profiling / time zsh -icx exit
#zmodload zsh/zprof

source $HOME/.config/zsh/zshrc-common
source $HOME/.config/zsh/zshrc-env
source $HOME/.config/zsh/zshrc-plugin
source $HOME/.config/zsh/zshrc-func
source $HOME/.config/zsh/zshrc-cheatcheet
source $HOME/.config/zsh/zshrc-alias.common

case "${OSTYPE}" in
  darwin*)
    source $HOME/.config/zsh/zshrc-alias.mac
    ;;
  linux*)
    source $HOME/.config/zsh/zshrc-alias.linux
    ;;
esac

# zshrc local setting
[ -f $HOME/.config/zsh/zshrc.local ] && source $HOME/.config/zsh/zshrc.local

# profiling
# if type zprof > /dev/null 2>&1; then
#     zprof |less
# fi
